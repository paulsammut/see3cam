<?xml version="1.0" encoding="UTF-8"?>

<launch>
    <group ns="stereo2">
        <node pkg="uvc_camera" type="uvc_stereo_node" name="stereo2_driver" output="screen">
            <param name="width" type="int" value="1280" />
            <param name="height" type="int" value="960" />
            <param name="fps" type="int" value="30" />
            <param name="frame"     type="string" value="stereo2_link" />
            <param name="frame_id"  type="string" value="stereo2_link" />
            <param name="left/device" type="string" value="/dev/video3" />
            <param name="right/device" type="string" value="/dev/video2" />
            <param name="left/camera_info_url" type="string" value="package://uvc_camera/config/stereo2/left.yaml" />
            <param name="right/camera_info_url" type="string" value="package://uvc_camera/config/stereo2/right.yaml" />
        </node>
    </group>
</launch>
